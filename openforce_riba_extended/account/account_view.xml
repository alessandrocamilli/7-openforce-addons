			<field name="inherit_id" ref="l10n_it_ricevute_bancarie.view_riba_da_emettere_tree"/>
			<field name="arch" type="xml">
			    
			    <xpath expr="//field[@name='iban']" position="replace">
				</xpath>
			    <xpath expr="//field[@name='riba']" position="after">
				    <field name="riba_amount_residual_emission" string="Residuo" sum="riba_amount_residual_emission"/>
				</xpath>
				<!-- 				
			    <xpath expr="//tree[@string='Issue Ri.Ba.']" position="attributes">
			        <attribute name="colors">red:date_maturity&gt;0</attribute>
				</xpath>
				 -->
			</field>
		</record>
    </data>
</openerp>
